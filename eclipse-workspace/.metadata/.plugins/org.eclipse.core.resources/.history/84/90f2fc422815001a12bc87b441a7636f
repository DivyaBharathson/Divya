package com.POM;

import org.openqa.selenium.WebElement;
import org.openqa.selenium.support.FindBy;
import org.openqa.selenium.support.PageFactory;

public class Amazon5MobilesPage1POM extends SeleniumBaseClass {

	@FindBy (id="twotabsearchtextbox")
	private WebElement enterMobile;
	
	@FindBy (xpath="(//img[@class='s-image'])[1]")
	private WebElement clickPhone1;
	
	@FindBy (xpath="(//img[@class='s-image'])[2]")
	private WebElement clickPhone2;
	
	@FindBy (xpath="(//img[@class='s-image'])[3]")
	private WebElement clickPhone3;
	
	@FindBy (xpath="(//img[@class='s-image'])[4]")
	private WebElement clickPhone4;
	
	@FindBy (xpath="(//img[@class='s-image'])[5]")
	private WebElement clickPhone5;
	
	public WebElement getEnterMobile() {
		return enterMobile;
	}

	public void setEnterMobile(WebElement enterMobile) {
		this.enterMobile = enterMobile;
	}

	public WebElement getClickPhone1() {
		return clickPhone1;
	}

	public void setClickPhone1(WebElement clickPhone1) {
		this.clickPhone1 = clickPhone1;
	}

	public WebElement getClickPhone2() {
		return clickPhone2;
	}

	public void setClickPhone2(WebElement clickPhone2) {
		this.clickPhone2 = clickPhone2;
	}

	public WebElement getClickPhone3() {
		return clickPhone3;
	}

	public void setClickPhone3(WebElement clickPhone3) {
		this.clickPhone3 = clickPhone3;
	}

	public WebElement getClickPhone4() {
		return clickPhone4;
	}

	public void setClickPhone4(WebElement clickPhone4) {
		this.clickPhone4 = clickPhone4;
	}

	public WebElement getClickPhone5() {
		return clickPhone5;
	}

	public void setClickPhone5(WebElement clickPhone5) {
		this.clickPhone5 = clickPhone5;
	}

	public Amazon5MobilesPage1POM() {
		PageFactory.initElements(driver,this);
	}
	
}
