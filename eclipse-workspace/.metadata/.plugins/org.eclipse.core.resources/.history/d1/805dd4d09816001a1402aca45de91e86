package com.POM;

import org.openqa.selenium.WebElement;
import org.openqa.selenium.support.FindBy;
import org.openqa.selenium.support.PageFactory;

public class FlipkartPage3POM extends SeleniumBaseClass{
	
	@FindBy (xpath="//button[@class='_2AkmmA _2Npkh4 _2MWPVK']")
	private WebElement firstOppo;

	
	public WebElement getFirstOppo() {
		return firstOppo;
	}

	public void setFirstOppo(WebElement firstOppo) {
		this.firstOppo = firstOppo;
	}
	
    public FlipkartPage3POM() {
		
		PageFactory.initElements(driver, this);
	}
	
	

}

